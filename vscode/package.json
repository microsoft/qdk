{
  "name": "qsharp-vscode",
  "displayName": "Q# (preview)",
  "description": "Q# Language Support",
  "version": "0.0.0",
  "publisher": "quantum",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages",
    "Notebooks"
  ],
  "browser": "./out/extension.js",
  "main": "./out/extension.js",
  "virtualWorkspaces": true,
  "activationEvents": [
    "onNotebook:jupyter"
  ],
  "contributes": {
    "languages": [
      {
        "id": "qsharp",
        "aliases": [
          "Q#",
          "qsharp"
        ],
        "extensions": [
          ".qs"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "qsharp",
        "scopeName": "source.qsharp",
        "path": "./syntaxes/qsharp.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "tsc:check": "node ../node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "build": "npm run tsc:check && node build.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/qsharp.git"
  },
  "license": "SEE LICENSE IN LICENSE.txt"
}
