{
  "name": "qsharp_jupyterlab",
  "version": "0.1.0",
  "description": "Q# extension for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/microsoft/qsharp",
  "bugs": {
    "url": "https://github.com/microsoft/qsharp/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Microsoft"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/qsharp.git"
  },
  "scripts": {
    "build": "npx -p typescript --no-install tsc && npm run build:labextension:dev",
    "build:prod": "rimraf lib tsconfig.tsbuildinfo && npm run build:lib && jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "npx -p typescript --no-install tsc --sourceMap",
    "install:extension": "npm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "npx -p typescript --no-install tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@codemirror/language": "6.6.0",
    "@codemirror/legacy-modes": "6.3.2",
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/codemirror": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@types/codemirror": "^5.60.7",
    "rimraf": "^3.0.2",
    "typescript": "^5.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "qsharp_jupyterlab/labextension"
  }
}
